{
    "save_workspace": "never",
    "load_workspace": false,
    "initial_working_directory": "~/armory",
    "posix_terminal_shell": "zsh",
    "default_project_location": "~/armory",
    "editor_keybindings": "vim",
    "restore_source_documents": false,
    "restore_last_project": false,
    "python_type": "system",
    "python_version": "3.13.1",
    "python_path": "~/.pyenv/versions/3.13.1/bin/python",
    "font_size_points": 12,
    "panes": {
        "quadrants": [
            "Console",
            "TabSet1",
            "TabSet2",
            "Source"
        ],
        "tabSet1": [
            "Environment",
            "Files",
            "Plots",
            "Packages",
            "Help",
            "Viewer",
            "Presentations",
            "Presentation"
        ],
        "tabSet2": [],
        "hiddenTabSet": [
            "History",
            "Connections",
            "Build",
            "VCS",
            "Tutorial"
        ],
        "console_left_on_top": true,
        "console_right_on_top": true,
        "additional_source_columns": 0
    },
    "editor_theme": "base16 Gruvbox dark, medium {rsthemes}",
    "rmd_viewer_type": "pane",
    "console_line_length_limit": 1080,
    "source_with_echo": true,
    "num_spaces_for_tab": 4,
    "syntax_color_console": true,
    "relative_line_numbers": true,
    "code_completion": "manual",
    "highlight_selected_line": true,
    "enable_text_drag": false,
    "highlight_r_function_calls": true,
    "rainbow_parentheses": true,
    "strip_trailing_whitespace": true,
    "tab_completion": false,
    "soft_wrap_r_files": true,
    "scroll_past_end_of_document": true,
    "vcs_enabled": false,
    "jobs_tab_visibility": "shown"
}